answer = input("Podaj datę urodzenia w formacie DD-MM-RRRR ")
answer = answer.split("-")
if len(answer) != 3:
    raise ValueError("Nieprawidłowy format daty urodzenia. Napisz literami dzień miesiąc i rok urodzenia i oddziel je myślnikami")
try:
  int(answer[1]) and int(answer[-1])
  month = int(answer[1])
  year = int(answer[-1])
  if month in range(1, 12) or year in range(1875, 2025):
    age_in_months = (2025 - year) * 12 + 8 - month
    print("masz " + str(age_in_months) + " miesięcy")
  else:
    raise ValueError("Nieprawidłowy wiek")
except TypeError:
  raise TypeError("Nieprawidłowy format daty urodzenia. Napisz literami dzień miesiąc i rok urodzenia i oddziel je myślnikami")
